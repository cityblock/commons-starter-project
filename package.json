    "rabbot": "^1.1.0",
    "raw-loader": "^0.5.1",
    "react": "15.6.1",
    "react-apollo": "^1.4.4",
    "react-dom": "15.6.1",
    "react-google-login": "^2.9.2",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-waypoint": "^7.0.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.0",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1",
    "typings-for-css-modules-loader": "^1.5.0",
    "url-loader": "^0.5.9",
