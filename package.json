    "basic-auth": "^1.1.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "cron": "^1.2.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "fetch-everywhere": "^1.0.5",
    "file-loader": "^0.11.2",
    "graphql": "^0.10.1",
    "graphql-custom-types": "^0.7.3",
    "graphql-server-express": "^0.8.2",
    "graphql-tools": "^1.0.0",
    "jsonwebtoken": "^7.4.1",
    "knex": "^0.13.0",
    "langs": "^2.0.0",
    "le_node": "^1.7.0",
    "lodash": "^4.17.4",
    "lost": "^8.0.0",
    "moment": "^2.18.1",
