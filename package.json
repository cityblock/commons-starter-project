    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.2",
    "typings-for-css-modules-loader": "^1.5.0",
