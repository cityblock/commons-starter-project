    "@types/node": "^7.0.31",
    "@types/optics-agent": "^1.1.1",
    "@types/qs": "^6.2.31",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.42",
    "@types/react-router-dom": "^4.0.3",
    "@types/react-router-redux": "^5.0.2",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-mock-store": "^0.0.8",
    "@types/uuid": "^3.0.0",
    "@types/validator": "^6.2.0",
    "@types/webpack": "^2.2.14",
