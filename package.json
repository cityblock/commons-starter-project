{
  "name": "commons",
  "version": "0.0.1",
  "description": "A Node.js app using Express and GraphQL",
  "license": "UNLICENSED",
  "repository": "https://github.com/cityblock/commons",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "stylelint": "stylelint 'app/**/*.css'",
    "typecheck": "tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf public server-compiled",
    "staging-db": "./scripts/aptible-psql.sh commons-staging",
    "prod-db": "./scripts/aptible-psql.sh commons-production",
    "copy-builder-staging-to-production": "aptible db:execute commons-production ./scripts/aptible-copy-builder-staging-to-production.sql",
    "update-fdw-production": "aptible db:execute commons-production ./scripts/setup-fdw-production.sql",
    "start": "NODE_ENV=production yarn forever -a -f ./server-compiled/server/index.js",
    "dev": "yarn check --verify-tree && yarn migrate && NODE_ENV=development nodemon --exec 'ts-node --transpileOnly -P tsconfig.server.json ./server/index.ts'",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && yarn run build-server && cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.js",
    "build-stats": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js --profile --json > stats.json",
    "build-server": "./node_modules/typescript/bin/tsc --project tsconfig.server.json",
    "prettier": "prettier --write \"{app,server,webpack,scripts}/**/*.{ts,tsx,js,css,graphql}\"",
    "test": "NODE_ENV=test node initialize-test-db.js && jest --runInBand --logHeapUsage",
    "test:frontend": "jest app/ --maxWorkers=2 --logHeapUsage",
    "test:api": "NODE_ENV=test node initialize-test-db.js && jest server/graphql server/handlers server/lib server/apis server/__tests__ --forceExit --maxWorkers=2 --logHeapUsage",
    "test:models": "NODE_ENV=test node initialize-test-db.js && jest server/models --maxWorkers=2 --logHeapUsage",
    "test:watch": "yarn test --watch",
    "test:notify": "yarn run test:watch --notify",
    "migrate": "knex migrate:latest --knexfile=server/models/knexfile.js",
    "migrate:rollback": "knex-migrate down --knexfile=server/models/knexfile.js",
    "prepush": "pretty-quick --staged && yarn run lint && yarn typecheck",
    "update-schema": "yarn check --verify-tree && ./scripts/generate-schema-types.sh",
    "codegen": "yarn check --verify-tree && ./scripts/codegen.sh",
    "typegen": "yarn check --verify-tree && ./scripts/generate-schema-types.sh && ./scripts/codegen.sh",
    "docker-prod:start": "docker-compose stop 2>/dev/null; docker-compose up",
    "docker-prod:restart": "docker-compose stop 2>/dev/null; docker-compose build && docker-compose up",
    "docker-prod:stop": "docker-compose stop",
    "docker-prod:migrate": "docker-compose exec web yarn migrate --env=production",
    "docker-prod:ts-node": "docker-compose exec web yarn ts-node -P tsconfig.server.json",
    "seed": "knex seed:run --knexfile=server/models/knexfile.js",
    "patients:add-admin-tasks-concern:dev": "NODE_ENV=development ts-node -P tsconfig.server.json ./scripts/add-admin-task-concern-to-patients.ts",
    "patients:add-admin-tasks-concern:production": "NODE_ENV=production ts-node -P tsconfig.server.json ./scripts/add-admin-task-concern-to-patients.ts",
    "import:icd:dev": "NODE_ENV=development ts-node -P tsconfig.server.json ./scripts/import-icd-ten-codes.ts",
    "import:icd:production": "NODE_ENV=production ts-node -P tsconfig.server.json ./scripts/import-icd-ten-codes.ts",
    "consume:computed-fields:dev": "NODE_ENV=development yarn forever -a ./server-compiled/server/consumers/computed-field-consumer.js",
    "consume:computed-fields:production": "NODE_ENV=production yarn forever -a ./server-compiled/server/consumers/computed-field-consumer.js",
    "consume:member-attribution:dev": "NODE_ENV=development yarn forever -a ./server-compiled/server/consumers/member-attribution-consumer.js",
    "consume:member-attribution:production": "NODE_ENV=production yarn forever -a ./server-compiled/server/consumers/member-attribution-consumer.js",
    "consume:scheduling:dev": "NODE_ENV=development yarn forever -a ./server-compiled/server/consumers/scheduling-consumer.js",
    "consume:scheduling:production": "NODE_ENV=production yarn forever -a ./server-compiled/server/consumers/scheduling-consumer.js",
    "consume:voicemail:dev": "NODE_ENV=development yarn forever -a ./server-compiled/server/consumers/voicemail-consumer.js",
    "consume:voicemail:production": "NODE_ENV=production yarn forever -a ./server-compiled/server/consumers/voicemail-consumer.js",
    "consume:mattermost:dev": "NODE_ENV=development yarn forever -a ./server-compiled/server/consumers/mattermost-consumer.js",
    "consume:mattermost:production": "NODE_ENV=production yarn forever -a ./server-compiled/server/consumers/mattermost-consumer.js",
    "computed-patient-status:update-all:dev": "NODE_ENV=development ts-node -P tsconfig.server.json ./scripts/update-computed-patient-statuses.ts",
    "computed-patient-status:update-all:production": "NODE_ENV=production ts-node -P tsconfig.server.json ./scripts/update-computed-patient-statuses.ts",
    "jobs:process-voicemail:dev": "NODE_ENV=development node ./server-compiled/server/jobs/process-voicemail.js",
    "jobs:process-voicemail:production": "NODE_ENV=production node ./server-compiled/server/jobs/process-voicemail.js"
  },
  "author": "Brennan Moore",
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "testEnvironmentOptions": {
      "IS_BUILDER_ENABLED": "true"
    },
    "testMatch": [
      "<rootDir>/app/**/?(*.)(spec|test).ts?(x)",
      "<rootDir>/server/**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./browser-mocks.js",
      "./enzyme-setup.js"
    ],
    "setupTestFrameworkScriptFile": "./jest-setup.js",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
