{
  "name": "commons",
  "version": "0.0.1",
  "description": "A Node.js app using Express and GraphQL",
  "repository": "https://github.com/sidewalklabs/commons",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --type-check --project tsconfig.json",
    "clean": "rimraf public server-compiled",
    "prod-db": "./scripts/prod-psql.sh",
    "start": "NODE_ENV=production nodemon --exec node ./server-compiled/index.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node ./server/index.ts",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && yarn run build-server && cross-env NODE_ENV=production GOOGLE_OAUTH_TOKEN=453019684022-7hpt0vfk9bjjc53pm2rl9f1vgmujkl68.apps.googleusercontent.com webpack -p --colors --display-error-details --config ./webpack/webpack.config.js",
    "build-server": "./node_modules/typescript/bin/tsc --project tsconfig.server.json",
    "prettier": "prettier --write \"./**/*.ts\"",
    "prettier:check": "prettier --debug-check \"./**/*.ts\"",
    "test": "jest --runInBand",
    "coverage": "jest --coverage --runInBand",
    "migrate": "knex migrate:latest --knexfile=server/models/knexfile.js",
    "test:watch": "yarn test -- --watch",
    "test:notify": "yarn run test:watch -- --notify",
    "prepush": "yarn run lint",
    "update-schema": "./scripts/generate-schema-types.sh",
    "codegen": "./scripts/codegen.sh",
    "low-priority": "NODE_ENV=production nodemon --exec ts-node ./server/rabbitmq/workers/low-priority-worker.ts",
    "high-priority": "NODE_ENV=production nodemon --exec ts-node ./server/rabbitmq/workers/high-priority-worker.ts",
    "cron": "NODE_ENV=production nodemon --exec ts-node ./server/config/cron.ts",
    "low-priority-dev": "NODE_ENV=development nodemon --exec ts-node ./server/rabbitmq/workers/low-priority-worker.ts",
    "high-priority-dev": "NODE_ENV=development nodemon --exec ts-node ./server/rabbitmq/workers/high-priority-worker.ts",
    "cron-dev": "NODE_ENV=development nodemon --exec ts-node ./server/config/cron.ts",
    "docker-prod:start": "docker-compose stop 2>/dev/null; docker-compose up",
    "docker-prod:restart": "docker-compose stop 2>/dev/null; docker-compose build && docker-compose up",
    "docker-prod:stop": "docker-compose stop",
    "docker-prod:migrate": "docker-compose exec web yarn migrate -- --env=production",
    "docker-prod:ts-node": "docker-compose exec web yarn ts-node",
    "seed": "knex seed:run --knexfile=server/models/knexfile.js",
    "user:add:dev": "NODE_ENV=development ts-node ./scripts/add-user.ts",
    "user:add:production": "NODE_ENV=production ts-node ./scripts/add-user.ts"
  },
  "author": "Brennan Moore",
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "server/**/*.{ts,tsx}",
      "!app/spec.helpers.ts",
      "!app/assets/**",
      "!app/**/*.css.d.ts",
      "!app/graphql/**",
      "!server/lib/capture-output.ts",
      "!server/apis/**/types.ts"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/app/**/?(*.)(spec|test).ts?(x)",
      "<rootDir>/server/**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "./browser-mocks.js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/jest": "^20.0.8",
    "@types/nock": "^8.2.1",
    "@types/react-test-renderer": "^15.5.4",
    "apollo-codegen": "^0.16.5",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "expect": "^21.0.0",
    "foreman": "^2.0.0",
    "gql2ts": "^1.2.1",
    "graphql-document-collector": "^0.1.3",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.1",
    "jest-css-modules": "^1.1.0",
    "jest-transform-graphql": "^2.1.0",
    "nock": "^9.0.14",
    "node-mocks-http": "^1.6.4",
    "prettier": "^1.6.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "webpack-dev-server": "^2.7.1"
  },
  "engines": {
    "node": "6.10.0"
  },
