{
  "name": "pokedex",
  "version": "0.0.1",
  "description": "A Node.js app using Express and GraphQL",
  "license": "UNLICENSED",
  "repository": "https://github.com/cityblock/pokedex",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "npx tslint --project tsconfig.json --config tslint.json",
    "stylelint": "stylelint 'app/**/*.css'",
    "typecheck": "npx tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf public server-compiled",
    "staging-db": "./scripts/aptible-psql.sh commons-staging",
    "prod-db": "./scripts/aptible-psql.sh commons-production",
    "copy-builder-staging-to-production": "aptible db:execute commons-production ./scripts/aptible-copy-builder-staging-to-production.sql",
    "update-fdw-production": "aptible db:execute commons-production ./scripts/setup-fdw-production.sql",
    "start": "NODE_ENV=production npx forever -a -f ./server-compiled/server/index.js",
    "dev": "npx migrate && NODE_ENV=development nodemon --exec 'ts-node --transpileOnly -P tsconfig.server.json ./server/index.ts'",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "npx run clean && npx run build-server && cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.js",
    "build-stats": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js --profile --json > stats.json",
    "build-server": "./node_modules/typescript/bin/tsc --project tsconfig.server.json",
    "prettier": "prettier --write \"{app,server,webpack,scripts}/**/*.{ts,tsx,js,css,graphql}\"",
    "test": "NODE_ENV=test node initialize-test-db.js && jest --runInBand --logHeapUsage",
    "test:frontend": "jest app/ --maxWorkers=2 --logHeapUsage",
    "test:api": "NODE_ENV=test node initialize-test-db.js && jest server/graphql --forceExit --maxWorkers=2 --logHeapUsage",
    "test:api-support": "NODE_ENV=test node initialize-test-db.js && jest server/handlers server/lib server/apis server/consumers server/__tests__ scripts/__tests__ --forceExit --maxWorkers=2 --logHeapUsage",
    "test:models": "NODE_ENV=test node initialize-test-db.js && jest server/models --maxWorkers=2 --logHeapUsage",
    "test:watch": "npx test --watch",
    "test:notify": "npx run test:watch --notify",
    "migrate": "knex migrate:latest --knexfile=server/models/knexfile.js",
    "migrate:rollback": "knex-migrate down --knexfile=server/models/knexfile.js",
    "update-schema": "./scripts/generate-schema-types.sh",
    "codegen": "./scripts/codegen.sh",
    "typegen": "./scripts/generate-schema-types.sh && ./scripts/codegen.sh",
    "docker-prod:start": "docker-compose stop 2>/dev/null; docker-compose up",
    "docker-prod:restart": "docker-compose stop 2>/dev/null; docker-compose build && docker-compose up",
    "docker-prod:stop": "docker-compose stop",
    "docker-prod:migrate": "docker-compose exec web npx migrate --env=production",
    "docker-prod:ts-node": "docker-compose exec web npx ts-node -P tsconfig.server.json",
    "seed": "knex seed:run --knexfile=server/models/knexfile.js"
  },
