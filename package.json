    "expect": "22.4.0",
    "foreman": "2.0.0",
    "gql2ts": "1.5.0",
    "graphql-document-collector": "0.2.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.2",
    "jest-transform-graphql": "2.1.0",
    "knex-migrate": "1.5.1",
    "nock": "9.2.3",
    "node-mocks-http": "1.6.7",
    "postcss": "6.0.19",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "react-test-renderer": "16.2.0",
    "redux-mock-store": "1.4.0",
    "stylefmt": "6.0.0",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "tslint-microsoft-contrib": "5.0.3",
    "webpack-dev-server": "3.1.1"
  },
  "engines": {
    "node": "8.9"
  },
