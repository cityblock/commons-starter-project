    "@types/base-64": "^0.1.2",
    "@types/basic-auth": "^1.1.2",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chart.js": "^2.7.3",
    "@types/classnames": "^2.2.3",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.1",
    "@types/express": "^4.0.39",
    "@types/fuse": "^2.5.2",
