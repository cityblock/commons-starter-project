{
  "name": "commons",
  "version": "0.0.1",
  "description": "A Node.js app using Express and GraphQL",
  "license": "UNLICENSED",
  "repository": "https://github.com/cityblock/commons",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "stylelint": "stylelint 'app/**/*.css'",
    "typecheck": "tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf public server-compiled",
    "prod-db": "./scripts/prod-psql.sh",
    "start": "NODE_ENV=production nodemon ./server-compiled/server/index.js",
    "dev": "yarn check --verify-tree && nodemon --exec ts-node ./server/index.ts",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && yarn run build-server && cross-env NODE_ENV=production GOOGLE_OAUTH_TOKEN=640963223283-n944uqosb4kae9cj99d35v6ut6n5bvlc.apps.googleusercontent.com webpack --colors --config ./webpack/webpack.config.js",
    "build-server": "./node_modules/typescript/bin/tsc --project tsconfig.server.json",
    "prettier": "prettier --write \"{app,server,webpack,scripts}/**/*.{ts,tsx,js,css,graphql}\"",
    "test": "NODE_ENV=test node initialize-test-db.js && jest --runInBand",
    "coverage": "NODE_ENV=test node initialize-test-db.js && jest --coverage --runInBand",
    "coverage:frontend": "jest --coverage app/",
    "coverage:api": "NODE_ENV=test node initialize-test-db.js && jest --coverage server/graphql server/handlers server/lib server/apis --runInBand",
    "coverage:models": "NODE_ENV=test node initialize-test-db.js && jest --coverage server/models --runInBand",
    "migrate": "knex migrate:latest --knexfile=server/models/knexfile.js",
    "migrate:rollback": "knex-migrate down --knexfile=server/models/knexfile.js",
    "test:watch": "yarn test --watch",
    "test:notify": "yarn run test:watch --notify",
    "prepush": "yarn run lint && yarn typecheck",
    "update-schema": "yarn check --verify-tree && ./scripts/generate-schema-types.sh",
    "codegen": "yarn check --verify-tree && ./scripts/codegen.sh",
    "typegen": "yarn check --verify-tree && ./scripts/generate-schema-types.sh && ./scripts/codegen.sh",
    "docker-prod:start": "docker-compose stop 2>/dev/null; docker-compose up",
    "docker-prod:restart": "docker-compose stop 2>/dev/null; docker-compose build && docker-compose up",
    "docker-prod:stop": "docker-compose stop",
    "docker-prod:migrate": "docker-compose exec web yarn migrate --env=production",
    "docker-prod:ts-node": "docker-compose exec web yarn ts-node",
    "seed": "knex seed:run --knexfile=server/models/knexfile.js",
    "user:add:dev": "NODE_ENV=development ts-node ./scripts/add-user.ts",
    "user:add:production": "NODE_ENV=production ts-node ./scripts/add-user.ts",
    "user:remove:dev": "NODE_ENV=development ts-node ./scripts/remove-user.ts",
    "user:remove:production": "NODE_ENV=production ts-node ./scripts/remove-user.ts",
    "patients:add-admin-tasks-concern:dev": "NODE_ENV=development ts-node ./scripts/add-admin-task-concern-to-patients.ts",
    "patients:add-admin-tasks-concern:production": "NODE_ENV=production ts-node ./scripts/add-admin-task-concern-to-patients.ts"
  },
  "author": "Brennan Moore",
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "server/**/*.{ts,tsx}",
      "!app/spec.helpers.ts",
      "!app/assets/**",
      "!app/**/*.css.d.ts",
      "!app/graphql/**",
      "!server/lib/capture-output.ts",
      "!server/apis/**/types.ts"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/app/**/?(*.)(spec|test).ts?(x)",
      "<rootDir>/server/**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./browser-mocks.js",
      "./enzyme-setup.js"
    ],
    "setupTestFrameworkScriptFile": "./jest-setup.js",
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
    "@types/nock": "^8.2.1",
    "@types/react-test-renderer": "^16.0.0",
