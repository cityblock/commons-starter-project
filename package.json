{
  "name": "pokedex",
  "version": "0.0.1",
  "description": "A Node.js app using Express and GraphQL",
  "license": "UNLICENSED",
  "repository": "https://github.com/cityblock/pokedex",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "npx tslint --project tsconfig.json --config tslint.json",
    "stylelint": "stylelint 'app/**/*.css'",
    "typecheck": "npx tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf public server-compiled",
    "start": "NODE_ENV=production npx forever -a -f ./server-compiled/server/index.js",
    "dev": "npm run migrate && NODE_ENV=development nodemon --exec 'ts-node --transpile-only -P tsconfig.server.json ./server/index.ts'",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "npx run clean && npx run build-server && cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.js",
    "build-server": "./node_modules/typescript/bin/tsc --project tsconfig.server.json",
    "prettier": "prettier --write \"{app,server,webpack,scripts}/**/*.{ts,tsx,js,css,graphql}\"",
    "test": "NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand --logHeapUsage",
    "knex": "ts-node -P tsconfig.server.json node_modules/.bin/knex",
    "test:watch": "npx test --watch",
    "test:notify": "npx run test:watch --notify",
    "migrate": "npm run knex -- migrate:latest --knexfile=server/models/knexfile.ts",
    "migrate:rollback": "npm run knex -- migrate:rollback --knexfile=server/models/knexfile.ts",
    "update-schema": "./scripts/generate-schema-types.sh",
    "codegen": "./scripts/codegen.sh",
    "typegen": "./scripts/generate-schema-types.sh && ./scripts/codegen.sh",
    "seed": "npm run knex -- seed:run --knexfile=server/models/knexfile.ts"
  },
  "author": "Brennan Moore",
  "devDependencies": {
    "@types/enzyme": "3.9.2",
    "@types/jest": "24.0.13",
    "@types/nock": "10.0.3",
